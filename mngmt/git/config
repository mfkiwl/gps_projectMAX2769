# config for the windows git
[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
	autocrlf=true
	safecrlf=true
[remote "origin"]
	fetch = +refs/heads/*:refs/remotes/origin/*
	url = git@github.com:venik/gps_project.git
	push = refs/heads/*:refs/heads/*
[branch "master"]
	remote = origin
	merge = refs/heads/master
